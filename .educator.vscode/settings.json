{
  "[ansible]": { "editor.defaultFormatter": "redhat.ansible" },
  "[javascript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[javascriptreact]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[json]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[jsonc]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[jsonl]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[markdown]": {
    "editor.wordWrap": "bounded",
    "editor.wordWrapColumn": 80,
    "editor.tabSize": 4
  },
  "[python]": { "editor.defaultFormatter": "charliermarsh.ruff" },
  "[shellscript]": { "editor.defaultFormatter": "foxundermoon.shell-format" },
  "[typescript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[typescriptreact]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[yaml]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.cursorStyle": "block",
  "editor.defaultFormatter": "foxundermoon.shell-format",
  "editor.formatOnSave": true,
  "editor.minimap.enabled": false,
  "files.autoSave": "onFocusChange",
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.openRepositoryInParentFolders": "never",
  "json.format.enable": false,
  "editor.fontLigatures": false,
  "editor.fontSize": 22,
  "editor.fontFamily": "'IBM Plex Mono', 'Droid Sans Mono', 'monospace', monospace",
  "debug.console.fontSize": 18,
  "scm.inputFontFamily": "editor",
  "scm.inputFontSize": 26,
  "terminal.integrated.fontSize": 20,
  "terminal.integrated.fontWeight": "600",
  "editor.fontWeight": "600",
  "window.zoomPerWindow": false,
  "explorer.compactFolders": false,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.inlineSuggest.enabled": false,
  "terminal.integrated.suggest.quickSuggestions": false,
  "workbench.colorCustomizations": {
    "editor.lineHighlightBackground": "#dfd",
    "editor.lineHighlightBorder": "#bdb",
    "editorCursor.background": "#fff",
    "editorCursor.foreground": "#000",
    "editorError.background": "#ffccccaa",
    "editorError.border": "#ffffff00",
    "editorError.foreground": "#ffcccc00",
    "editorWarning.background": "#ffffccaa",
    "editorWarning.border": "#ffffff00",
    "editorWarning.foreground": "#ffffff00",
    "icon.foreground": "#aaa",
    "list.activeSelectionBackground": "#dfd",
    "list.activeSelectionForeground": "#00f",
    "list.activeSelectionIconForeground": "#aaa",
    "list.focusedBackground": "#dfd",
    "list.inactiveSelectionBackground": "#dfd",
    "list.inactiveSelectionForeground": "#00f",
    "sideBar.foreground": "#000",
    "quickInputList.focusBackground": "#eff",
    "quickInputList.focusForeground": "#aaa",
    "quickInputList.focusIconForeground": "#aaa",
    "list.hoverBackground": "#dfd",
    "list.hoverForeground": "#00f",
    "list.hoverIconForeground": "#00f"
  },
  "workbench.editor.showIcons": false,
  "explorer.decorations.colors": false,
  "editor.colorDecorators": false,
  "workbench.iconTheme": null,
  "breadcrumbs.enabled": false,
  "editor.cursorSurroundingLines": 3,
  "screencastMode.keyboardOptions": {
    "showSingleEditorCursorMoves": false,
    "showKeys": false
  },
  "screencastMode.keyboardOverlayTimeout": 2000,
  "screencastMode.mouseIndicatorSize": 60,
  "screencastMode.verticalOffset": 10,
  "terminal.integrated.defaultProfile.linux": "BashPresentation",
  "terminal.integrated.profiles.linux": {
    "BashPresentation": {
      "args": ["--init-file", "./.educator.shared/.bashrc"],
      "path": "bash"
    }
  },
  "workbench.colorTheme": "Default High Contrast Light",
  "terminal.integrated.minimumContrastRatio": 7,
  "workbench.statusBar.visible": false,
  "workbench.activityBar.location": "hidden",
  "auto-run-command.rules": [
    {
      "condition": "always",
      "command": "workbench.action.toggleScreencastMode"
    }
  ],
  "explorer.excludeGitIgnore": true,
  "files.exclude": {
    "**/ignored*": false
  },
  "vscode_custom_css.imports": []
}
